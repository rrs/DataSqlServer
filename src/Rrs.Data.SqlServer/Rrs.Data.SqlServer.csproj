<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>2.0.0</Version>
    <TargetFrameworks>net452;netstandard20</TargetFrameworks>
    <Description>Rrs.Data.SqlServer Class Library</Description>
    <Authors>Robert Snipe</Authors>
    <Copyright>Copyright (c) 2018 Robert Snipe</Copyright>
    <AssemblyName>Rrs.Data.SqlServer</AssemblyName>
    <PackageId>Rrs.Data.SqlServer</PackageId>
    <PackageTags>Data;SqlServer;net452</PackageTags>
    <PackageProjectUrl>https://github.com/rrs/DataSqlServer</PackageProjectUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Company>rrs</Company>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)'=='netstandard20'">
    <PackageReference Include="System.Data.SqlClient" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Rrs.Data" Version="2.0.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
  </ItemGroup>

</Project>
